{
  "name": "adarastyle-ecommerce",
  "version": "1.0.0",
  "description": "Tienda de bolsos completamente parametrizable con Strapi 5 y Angular",
  "private": true,
  "scripts": {
    "setup": "pnpm install",
    "setup:backend": "pnpm --filter adarastyle-backend install",
    "setup:frontend": "pnpm --filter adarastyle-frontend install",
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "dev:backend": "pnpm --filter adarastyle-backend run develop",
    "dev:frontend": "pnpm --filter adarastyle-frontend run start",
    "build": "pnpm run build:backend && pnpm run build:frontend",
    "build:backend": "pnpm --filter adarastyle-backend run build",
    "build:frontend": "pnpm --filter adarastyle-frontend run build",
    "seed": "pnpm --filter adarastyle-backend run seed"
  },
  "keywords": [
    "ecommerce",
    "strapi",
    "angular",
    "tienda",
    "bolsos",
    "parametrizable"
  ],
  "author": "AdaraStyle",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.1.0"
  },
  "engines": {
    "node": ">=20.19.5",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
